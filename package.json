{
  "private": true,
  "name": "tpb-ui",
  "version": "0.0.0",
  "dependencies": {
    "angular": "^1.3.0",
    "browserify": "^6.0.3",
    "uglify-js": "^2.4.15"
  },
  "scripts": {
    "clean-chrome": "rm -rf dist-chrome",
    "clean": "npm run clean-chrome",
    "copy-chrome": "mkdir dist-chrome && cp -rf src/* dist-chrome",
    "js-chrome": "browserify src/js.js | uglifyjs > dist-chrome/js.js",
    "rm-js-chrome": "rm dist-chrome/util.js dist-chrome/tpb-ui.js",
    "build-chrome": "npm run clean-chrome && npm run copy-chrome && npm run js-chrome && npm run rm-js-chrome",
    "build": "npm run build-chrome",
    "install": "npm run build"
  }
}
