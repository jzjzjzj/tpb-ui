{
  "name": "TPB UI - The Pirate Bay Special",
  "short_name": "TPB UI",
  "description": "Improved user interface of ThePirateBay (tpb).",
  "version": "1.5.1",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "app.js",
        "start.js"
      ],
      "css": [
        "css.css"
      ],
      "matches": [
        "https://thepiratebay.se/top/all",
        "https://thepiratebay.se/top/200",
        "https://thepiratebay.se/top/48h200",
        "https://thepiratebay.se/top/201",
        "https://thepiratebay.se/top/205",
        "https://thepiratebay.se/top/207",
        "https://thepiratebay.se/top/208"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "app.js",
        "idle.js"
      ],
      "matches": [
        "https://thepiratebay.se/top/all",
        "https://thepiratebay.se/top/200",
        "https://thepiratebay.se/top/48h200",
        "https://thepiratebay.se/top/201",
        "https://thepiratebay.se/top/205",
        "https://thepiratebay.se/top/207",
        "https://thepiratebay.se/top/208"
      ]
    }
  ],
  "web_accessible_resources": [
    "table-template.html"
  ]
}
