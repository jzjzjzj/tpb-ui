{
  "name": "TPB UI - The Pirate Bay Special",
  "short_name": "TPB UI",
  "description": "Improved user interface of The Pirate Bay.",
  "version": "1.0.0",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts": [{
    "js": [
      "js.js"
    ],
    "css": [
      "css.css"
    ],
    "matches": [
      "http://thepiratebay.se/top/all"
    ]
  }],
  "web_accessible_resources": [
    "table-template.html"
  ]
}
